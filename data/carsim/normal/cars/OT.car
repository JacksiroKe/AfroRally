drive = AWD
# all positions have x,y,z dir going + to: right, front, up

[ steering ]
max-angle = 26
angular-damping = 0.0

[ rot_drag ]
roll  = 200.0
pitch = 400.0
yaw   = 500.0
yaw2  = 2.0

[ model_ofs ]
rot_fix = on
boost-y = -2.2
boost-z = 1.05
boost-x = 0.4
exhaust-x = 2.2
exhaust-y = 0.45
exhaust-z = 0.56
exhaust-mirror-second = 0


#  collision shape params
#---------------------------
[ collision ]
com_ofs_H = 0.0
radius = 0.304
width = 0.70
height = 1.0
posLrear = -3.4
posLfront = 1.9
offsetL = -0.22
offsetW = 0.0
offsetH = 0.5
start-offsetY = 1.1
fluidTrigH = 0.0
friction = 0.4


#  engine
#---------------------------
[ engine ]
sound = engine
position = 0.0, 1.1, 0.9
mass = 180.0
rpm-limit = 2700
inertia = 0.30
start-rpm = 500
stall-rpm = 200
fuel-consumption = 1e-9
friction = 230
torque-val-mul = 1.0
torque-curve-00 = 500, 280
torque-curve-01 = 800, 420
torque-curve-02 = 1000, 480
torque-curve-03 = 1500, 530
torque-curve-04 = 2000, 550
torque-curve-05 = 2500, 530
torque-curve-06 = 3000, 510
torque-curve-07 = 3500, 470
real-pow-tq-mul = 1.2
sound-vol-mul = 2.0

[ clutch ]
max-torque-mul = 1.1

#  gearbox
#---------------------------
[ transmission ]
gears = 6
gear-ratio-r = -4.55
gear-ratio-1 = 5.02
gear-ratio-2 = 2.65
gear-ratio-3 = 1.52
gear-ratio-4 = 1.00
gear-ratio-5 = 0.60
gear-ratio-6 = 0.36
shift-delay = 0.2

[ differential ]
