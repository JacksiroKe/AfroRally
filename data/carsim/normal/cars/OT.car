anti-slip = 900.0

[ fuel-tank ]
position = 0.0, 0.26, 0.56
capacity = 60.0
volume = 60.0
fuel-density = 1.0


#  suspension
#---------------------------
[ suspension-front ]
spring-constant = 52000
bounce = 6500
rebound = 5000
travel = 0.62
camber = -1.33
caster = 6.12
toe = 0.0
anti-roll = 4000
factors-file = first

[ suspension-rear ]
spring-constant = 48000
bounce = 6000
rebound = 4000
travel = 0.62
camber = -0.45
caster = 0.28
toe = 0.0
anti-roll = 3000
