drive = AWD
version = 2
# all positions have x,y,z dir going + to: right, front, up

[ steering ]
max-angle = 30
angular-damping = 0.0
flip-pow-mul = 2

[ rot_drag ]
roll  = 200.0
pitch = 400.0
yaw   = 700.0
yaw2  = 2.0

[ model_ofs ]
rot_fix = on
boost-x = -0.3
boost-z = 0.09
boost-y = -0.26
boost-size-z = 0
boost-name = BoostRed
exhaust-x = 2.2
exhaust-y = 0.45
exhaust-z = 0.56
exhaust-mirror-second = 0


#  collision shape params
#---------------------------
[ collision ]
com_ofs_L = 0.15
com_ofs_H = -0.05
radius = 0.304
width = 1.2
height = 0.6
posLrear = -2.2
posLfront = 2.3
offsetL = 0.0
offsetW = 0.03
offsetH = 0.15
start-offsetY = 0.79
fluidTrigH = -0.0
friction = 0.2


#  engine
#---------------------------
[ engine ]
position = 0.0, 2.0, 0.2
mass = 180.0
rpm-limit = 7300
inertia = 0.40
start-rpm = 1000
stall-rpm = 400
fuel-consumption = 1e-9
friction = 130
torque-val-mul = 1.25
torque-curve-00 = 1000, 400
torque-curve-01 = 1500, 460
torque-curve-02 = 2000, 500
torque-curve-03 = 2500, 540
torque-curve-04 = 3000, 575
torque-curve-05 = 3500, 623
torque-curve-06 = 4000, 644
torque-curve-07 = 4500, 625
torque-curve-08 = 5000, 579
